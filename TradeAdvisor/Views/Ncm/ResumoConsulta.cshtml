        RequestPostGrafico('/services/ConsultaDadosGraficos.asmx/ConsultaDadosPorNcmValor', '', 'produtos_1_1', 'Valor', false);
        RequestPostGrafico('/services/ConsultaDadosGraficos.asmx/ConsultaDadosPorNcmQtde', '', 'produtos_1_2', 'Quantidade', true);

        RequestPostGrafico('/services/ConsultaDadosGraficos.asmx/ConsultaDadosPorPaisAquisicaoValor', '', 'produtos_2_1', 'Valor', false);
        RequestPostGrafico('/services/ConsultaDadosGraficos.asmx/ConsultaDadosPorPaisAquisicaoQtde', '', 'produtos_2_2', 'Quantidade', true);

        RequestPostGrafico('/services/ConsultaDadosGraficos.asmx/ConsultaDadosPorPaisOrigemValor', '', 'produtos_3_1', 'Valor', false);
        RequestPostGrafico('/services/ConsultaDadosGraficos.asmx/ConsultaDadosPorPaisOrigemQtde', '', 'produtos_3_2', 'Quantidade', true);
    });   

</script>

<script type="text/javascript">
    function RequestPostGrafico(url, parametros, nm_grafico, text, isqtde) {
        $.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            url: url,
            data: parametros,
            dataType: "json",
            success: function (Result) {
                Result = Result.d;
                var dataGrafNcm = [];
                for (var i in Result) {

                    var serie = {};
                    serie.name = Result[i].name;
                    if (isqtde)
                    {
                        serie.value = Result[i].qtde;
                        serie.colorValue = Result[i].qtde;
                    }
                    else
                    {
                        serie.value = Result[i].valor;
                        serie.colorValue = Result[i].valor;
                    }                   

                    dataGrafNcm.push(serie);
                }
                if (isqtde)
                    DreawChartTreemap(dataGrafNcm, nm_grafico, text, '$');
                else
                    DreawChartTreemap(dataGrafNcm, nm_grafico, text, '');
            },
            error: function (Result) {
                alert("Houve um erro ao cerregar dados para o gráfico.");
            }
        });
    }

    function DreawChartTreemap(series, nm_grafico, text, simbolo) {
        $('#' + nm_grafico).highcharts({
            colorAxis: {
                minColor: '#007FFF',
                maxColor: '#C0D9D9'
            },
            tooltip: {
                formatter: function () {
                    return '<b>' + this.point.name + '</b>: ' + ' ' + simbolo + Highcharts.numberFormat(this.point.value, 0);
                }
            },
            series: [{
                type: "treemap",
                layoutAlgorithm: 'squarified',
                data: series
            }],
            title: {
                text: text
            }
        });
    }
</script>